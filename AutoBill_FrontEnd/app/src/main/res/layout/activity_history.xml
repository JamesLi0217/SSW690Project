<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".History">
    <TextView
        android:id="@+id/H_text_view1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="60dp"
        android:layout_margin="20dp"
        android:text="History"
        android:textSize="30dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="#22B4D8"/>
    <Button
        android:id="@+id/getHistory"
        android:layout_below="@id/H_text_view1"
        android:layout_height="50dp"
        android:layout_width="match_parent"
        android:text="@string/app_name"/>
    <TextView
        android:id="@+id/history"
        android:layout_below="@id/getHistory"
        android:layout_width="match_parent"
        android:layout_height="200dp" />
    <Button
        android:id="@+id/history2"
        android:layout_below="@id/getHistory"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Histroy"
        android:textAllCaps="false"/>
    <Button
        android:id="@+id/test_listview"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/history"
        android:text="Get ListView"/>



    

</RelativeLayout>