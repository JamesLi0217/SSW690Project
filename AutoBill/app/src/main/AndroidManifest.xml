        <activity android:name=".EditprofileActivity" />
        <activity android:name=".SidesettingActivity" />
        <activity android:name=".ReportActivity" />
        <activity android:name=".AddlabelActivity" />
        <activity android:name=".AddreamrkActivity" />
        <activity android:name=".ProfileSettingsActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".AccountsetActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".GeneralsetActivity" />
        <activity android:name=".createbillActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>